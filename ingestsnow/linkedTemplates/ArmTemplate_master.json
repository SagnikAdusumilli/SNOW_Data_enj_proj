{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ingestsnow"
		},
		"ls_adls_conn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_conn'"
		},
		"ls_snow_conn_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_snow_conn'"
		},
		"ls_adls_conn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://snowadlsacc.dfs.core.windows.net/"
		},
		"ls_conn_admin_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://dev226103.service-now.com/"
		},
		"ls_conn_admin_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "admin"
		},
		"ls_snow_conn_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://tdbfgdev.service-now.com/"
		},
		"ls_snow_conn_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "ws.auto.td.eGRC"
		},
		"snow_akv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sagniksnowakv.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ingestsnow_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_conn_accountKey": {
						"value": "[parameters('ls_adls_conn_accountKey')]"
					},
					"ls_snow_conn_password": {
						"value": "[parameters('ls_snow_conn_password')]"
					},
					"ls_adls_conn_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_conn_properties_typeProperties_url')]"
					},
					"ls_snow_conn_properties_typeProperties_endpoint": {
						"value": "[parameters('ls_snow_conn_properties_typeProperties_endpoint')]"
					},
					"ls_snow_conn_properties_typeProperties_username": {
						"value": "[parameters('ls_snow_conn_properties_typeProperties_username')]"
					},
					"snow_akv_properties_typeProperties_baseUrl": {
						"value": "[parameters('snow_akv_properties_typeProperties_baseUrl')]"
					},
					"ls_conn_admin_properties_typeProperties_endpoint": {
						"value": "[parameters('ls_conn_admin_properties_typeProperties_endpoint')]"
					},
					"ls_conn_admin_properties_typeProperties_username": {
						"value": "[parameters('ls_conn_admin_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}